@section scripts {
<script type="text/javascript">
    $(function(){
        var db = fruit.databind.root;
        $.extend(db.options, {
            add: function(){
                fruit.ajax({
                        url:'/api/Mms/Test/NewBillNo',
                        success:function(data) {
                            parent.showDocTab('', '测试菜单明细', '/mms/test/edit/' + data);
                        }
                    })
            }
        });
    });
</script>
}
<div class="z-toolbar">

    <a class="easyui-linkbutton" iconcls="icon-arrow_refresh" plain="true" data-bind="click:'refresh'">刷新</a>
    <a class="easyui-linkbutton" iconcls="icon-add" plain="true" data-bind="click:'add'">新增</a>
    <a class="easyui-linkbutton" iconcls="icon-edit" plain="true" data-bind="click:'edit'">编辑</a>
    <a class="easyui-linkbutton" iconcls="icon-cross" plain="true" data-bind="click:'delete'">删除</a>
    <a class="easyui-splitbutton" plain="true" data-options="iconCls:'icon-download'" suffix="xls" data-bind="click:'download'">导出</a>
</div>
<table id="grid" data-bind="datagrid" data-options="fixSize:{w:4, h:40}, rownumbers:true, method:'get', url:'/api/mms/test', pagination:true, pageSize:20">
<thead>
<tr>
    <th field="BillNo" data-options="editor:{type:'validatebox',options:{required:true}}">BillNo    </th>
    <th field="BillDate" data-options="editor:{type:'datebox',options:{required:false}}">BillDate    </th>
    <th field="DoPerson">DoPerson    </th>
    <th field="WarehouseName">WarehouseName    </th>
    <th field="Years">Years    </th>
    <th field="Months">Months    </th>
    <th field="MaterialType">MaterialType    </th>
    <th field="BookMoney">BookMoney    </th>
    <th field="OperateMoney">OperateMoney    </th>
    <th field="Remark">Remark    </th>
</tr>
</thead>
</table>
